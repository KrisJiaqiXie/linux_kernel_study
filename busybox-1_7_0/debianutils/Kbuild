lib-y:=
lib-$(CONFIG_MKTEMP)            += mktemp.o
lib-$(CONFIG_PIPE_PROGRESS)     += pipe_progress.o
lib-$(CONFIG_RUN_PARTS)         += run_parts.o
lib-$(CONFIG_START_STOP_DAEMON) += start_stop_daemon.o
lib-$(CONFIG_WHICH)             += which.o
