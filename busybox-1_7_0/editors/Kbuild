lib-y:=
lib-$(CONFIG_AWK)       += awk.o
lib-$(CONFIG_CMP)       += cmp.o
lib-$(CONFIG_DIFF)      += diff.o
lib-$(CONFIG_ED)        += ed.o
lib-$(CONFIG_PATCH)     += patch.o
lib-$(CONFIG_SED)       += sed.o
lib-$(CONFIG_VI)        += vi.o
